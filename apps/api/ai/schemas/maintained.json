{
    "name": "log_well_maintained",
    "description": "Record a WELL-MAINTAINED element for QA metrics",
    "strict": true,
    "parameters": {
      "type": "object",
      "properties": {
        "event_id": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "inspected_at": {
          "type": "string",
          "format": "date-time"
        },
        "category": {
          "type": "string",
          "enum": []
        },
        "status": {
            "type": "string",
            "enum": [
                "good",
                "acceptable",
                "bad"
            ]
        },
        "condition_score": {
          "type": "number",
          "minimum": 0,
          "maximum": 100
        }
      },
      "required": [
        "event_id",
        "description",
        "inspected_at",
        "category",
        "status",
        "condition_score"
      ]
    }
}